---
import "../style/scss/project.scss"

const {
    Image="url(https://AmyCur.github.io/tree.jpg)",
    Heading="Heading", 
    Height="10vh",
    Link="/"
} = Astro.props;
---
<a href={Link}>
    <div id="project" style=`background-image: ${Image}; height: ${Height}`>
        <h1>{Heading}</h1>
    </div>
</a>

